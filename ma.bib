 keywords = {Aerodynamics;Aircraft;Commercial products;Economic analysis;Electric aircrafts;Electric propulsion;Electrical components;Fixed wings;Multi-disciplinary designs;Multi-disciplinary optimizations;Performance parameters;Physics-based analysis;Surveys;Technology factors},
 pages = {1--19},
 volume = {104},
 issn = {03760421},
 journal = {Progress in Aerospace Sciences},
 doi = {10.1016/j.paerosci.2018.06.004},
 file = {Brelje, Martins 2019 - Electric, hybrid:Attachments/Brelje, Martins 2019 - Electric, hybrid.pdf:application/pdf}
